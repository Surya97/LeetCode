package Leetcode;

public class RemoveDuplicatedFromSortedArray {
	public int removeDuplicates(int[] nums) {
		if (nums.length == 0) {
			return 0;
		}
		int cptr = 1;
		int i = 1;
		int count = 1;
		int temp = nums[0];
		while (i < nums.length) {
			while (i < nums.length && nums[i] == temp) {
				i += 1;
			}
			if (i >= nums.length) {
				break;
			}
			temp = nums[cptr];
			nums[cptr] = nums[i];
			nums[i] = temp;

			temp = nums[cptr];
			cptr += 1;
			count += 1;
			i += 1;
		}
		// for(i = 0; i< nums.length; i++){
		// System.out.print(nums[i] + " ");
		// }
		return count;
	}
}
