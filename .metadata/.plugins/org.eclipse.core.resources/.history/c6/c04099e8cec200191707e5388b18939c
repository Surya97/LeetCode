package Sorting;

class Node{
	int data;
	Node next;
	
	public Node(int data) {
		this.data = data;
		this.next = null;
	}
	
}

class LinkedList{
	Node head;
	
	public void addNode(int data) {
		Node tempNode = new Node(data);
		if(head == null) {
			head = tempNode;
		}
		else {
			Node temp = head;
			while(temp.next!=null) {
				temp = temp.next;
			}
			temp.next = tempNode;
		}
	}
}

public class LinkedListMergeSort {
	
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
